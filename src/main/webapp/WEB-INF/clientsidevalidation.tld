<jspversion>1.1</jspversion>
<shortname>Check taglib</shortname>
<uri>http://www.servletsuite.com/servlets/checktag</uri>
<info>Client side validation</info> 

<tag>
<name>required</name>
<tagclass>com.cj.check.requiredTag</tagclass>
